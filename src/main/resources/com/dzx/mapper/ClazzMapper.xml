<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hnu.dao.ClazzMapper">


    <select id="selectByPrimaryKey" resultType="com.hnu.model.Clazz">
        SELECT *
        FROM clazz
        WHERE id = #{id}
    </select>

</mapper>